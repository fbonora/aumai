version: '3'
services:
  web:
    image: fbonora/aumai:1.0
    ports:
      - "5000:5000"
  mysql:
    image: mysql
    environment:
      MYSQL_DATABASE: aumaiDB
      MYSQL_ROOT_PASSWORD: root
      MYSQL_ROOT_HOST: 0.0.0.0
      MYSQL_USER: root
      MYSQL_PASSWORD: aumai123!
    ports:
      - "3306:3306"
